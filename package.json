{
  "name": "nuxt-keycloak",
  "version": "0.0.1",
  "description": "Comprehensive Keycloak authentication module for Nuxt 4 with SSR support, auto token refresh, role-based guards, and TypeScript",
  "repository": "productdevbook/nuxt-keycloak",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.js"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ],
      "./server": [
        "./dist/server/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "npm run dev:prepare && nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "bumpp": "bumpp package.json",
    "release": "pnpm build && pnpm bumpp && pnpm publish --no-git-checks --access public"
  },
  "dependencies": {
    "@nuxt/kit": "^4.1.3",
    "defu": "^6.1.4",
    "jose": "^6.1.0",
    "keycloak-js": "^26.2.1"
  },
  "devDependencies": {
    "bumpp": "^10.3.1",
    "@nuxt/devtools": "^2.6.5",
    "@nuxt/eslint-config": "^1.9.0",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.1.3",
    "@nuxt/test-utils": "^3.19.2",
    "@nuxtjs/tailwindcss": "7.0.0-beta.1",
    "@types/node": "latest",
    "changelogen": "^0.6.2",
    "eslint": "^9.38.0",
    "nuxt": "^4.1.3",
    "typescript": "~5.9.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.1"
  },
  "resolutions": {
    "nuxt-keycloak": "link:."
  }
}